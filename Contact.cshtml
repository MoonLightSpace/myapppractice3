<!doctype html>
<!-- Website Template by freewebsitetemplates.com -->
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Contact - Mustache Enthusiast</title>
    <link rel="stylesheet" type="text/css" href="~/css/site.css">
    <script type="text/javascript" src="~/js/site.js"></script>
</head>
<body>
    <div id="header">
        <a href="index.html" class="logo">
            <img src="~/images/logo.jpg" alt="">
        </a>

        <ul id="navigation">
            <li>
                @Html.ActionLink("Home", "Index")
            </li>
            <li>
                @Html.ActionLink("About", "About")
            </li>
            <li>
                @Html.ActionLink("Gallery", "gallery")
            </li>
            <li>
                @Html.ActionLink("Blog", "Blog")
            </li>
            <li class="selected">
                @Html.ActionLink("Contact", "Contact")
            </li>
        </ul>

    </div>
    <div id="body">
        <h1><span>Contact Us</span></h1>
        <form action="contact.html">
            <input type="text" name="fname" id="fname" value= " Write your name">
            <input type="text" name="email" id="email" value="Write your email">
            <textarea name="message" id="message">message</textarea>
            <input type="submit" name="send" id="send" value="send">
        </form>
    </div>
    <div id="footer">
        <div>
            <p>&copy; 2019 by Mustacchio. All rights reserved.</p>
            <ul>
                <li>
                    <img src="~/images/icon-googleplus.jpg" alt="">
                </li>
                <li>
                    <img src="~/images/icon-pinterest.jpg" alt="">
                </li>
                <li>
                    <img src="~/images/icon-twitter.jpg" alt="">
                </li>
                <li>
                    <img src="~/images/icon-facebook.jpg" alt="">
                </li>
            </ul>
        </div>
    </div>
</body>
</html>
